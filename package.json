{
  "name": "chrome-extension-nexus-iq",
  "version": "1.9.4",
  "description": "Sonatype Nexus IQ Evaluation - Scan Open Source Repositories for known Vulnerabilities.",
  "main": "background.js",
  "scripts": {
    "test": "jest --runInBand --testNamePattern=UnitTest --watch",
    "test:UnitTest": "jest --testNamePattern=UnitTest",
    "test:IntegrationTest": "jest --runInBand --testNamePattern=IntegrationTest",
    "test:EndToEnd": "jest --runInBand",
    "semantic-release": "semantic-release",
    "doc": "jsdoc -c jsdoc.json",
    "scan-ossi": "auditjs ossi",
    "scan-iq": "auditjs iq -s develop -a chrome-extension-nexus-iq -u admin -h http://iq-server:8070",
    "dev": "webextension-toolbox dev -s src",
    "build": "webextension-toolbox build -s src",
    "lint": "eslint ./app/scripts/**/*",
    "lint:fix": "eslint ./app/scripts/**/* --fix"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "setupFiles": [
      "jest-webextension-mock"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype-nexus-community/nexus-iq-chrome-extension.git"
  },
  "keywords": [
    "IQ",
    "Validation",
    "Nexus",
    "Sonatype",
    "Lifecycle"
  ],
  "author": "Cameron Townshend",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ctownshend/chrome-extension-nexus-iq/issues"
  },
  "homepage": "https://github.com/ctownshend/chrome-extension-nexus-iq#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@types/chai": "^4.2.11",
    "@types/chrome": "0.0.101",
    "@types/cookies": "^0.7.4",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.3.33",
    "@types/jqueryui": "^1.12.10",
    "@types/node": "^13.9.3",
    "@types/puppeteer": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "assert": "^2.0.0",
    "auditjs": "^4.0.14",
    "ava": "^3.8.2",
    "axios-mock-adapter": "^1.18.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "jest-webextension-mock": "^3.5.0",
    "jsdoc": "^3.6.4",
    "merge": "^1.2.1",
    "path": "^0.12.7",
    "puppeteer": "^3.3.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "semantic-release": "^17.0.7",
    "semantic-release-chrome": "^1.1.3",
    "semver": "^7.3.2",
    "sinon": "^9.0.2",
    "source-map-loader": "^1.0.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^6.2.2",
    "ts-sinon": "^1.2.0",
    "typescript": "^3.9.5",
    "webextension-toolbox": "^3.0.0",
    "webpack": "~4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "~4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@material/dialog": "^5.1.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1"
  }
}
