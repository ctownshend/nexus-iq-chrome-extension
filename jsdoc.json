{
  "source": {
    "include": [
      "src/Scripts/background.js",
      "src/Scripts/content.js",
      "src/Scripts/options.js",
      "src/Scripts/popup.js",
      "src/Scripts/utils.js"
    ]
  },
  "exclude": [
    "src/Scripts/lib/jquery.min.js",
    "src/Scripts/lib/jquery.event.drag-2.3.0.js",
    "src/Scripts/lib/jquery-ui-1.12.1",
    "src/Scripts/lib/jquery-ui-themes-1.12.1",
    "src/Scripts/lib/appcheck.js",
    "src/Scripts/lib/axios.min.js",
    "src/Scripts/lib/protovis.min.js"
  ],
  "includePattern": ".+\\.js(doc|x)?$",
  "excludePattern": "(node_modules|docs|jquery|lib)",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },

  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
